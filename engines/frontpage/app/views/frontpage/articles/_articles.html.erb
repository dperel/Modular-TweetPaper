<div class="articleHolder" id="articleHolder">
  <% @articles.map do |article|%>
    <div class="child">
      <p><h1><%= article.title.upcase%></h1></p>
        <%if article.picture_url %>
          <p><%= image_tag article.picture_url %></p>
        <%end%>
      <p><%= article.tweet_1 %><br /></p>
      <p><%= article.tweet_2 %><br /></p>
      <p><%= article.tweet_3 %><br /></p>
      <p><%= article.tweet_4 %><br /></p>
      <p><%= article.tweet_5 %><br /></p>
      <%= link_to("Save", star_article_path(article))%> |
      <%= link_to("Delete", article_path(article), :method => :delete)%><br />
    </div>
  <%end%>
</div>
